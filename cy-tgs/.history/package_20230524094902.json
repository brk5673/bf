{
  "name": "api-test",
  "version": "1.0.0",
  "description": "api test tgs web service",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open --e2e",
    "cy:run": "cypress run",
    "test": "echo \"Error: no test specified\" && exit 1",
    "cy:runChrome": "cypress run --browser chrome --spec cypress/e2e/login.cy.js",
    "cy:contactsConfig": "cypress run --browser chrome --spec cypress/e2e/SPAC/Mantenimiento/Desvio_de_inyecciones/configuracion_contactos.cy.js",
    "cy:consultaInyecciones": "cypress run --browser chrome --spec cypress/e2e/SPAC/Control/consulta_desvio_inyecciones.cy.js",
    "api-regression-allure": "cypress run --browser chrome --spec cypress/e2e/**.js --env allure=true",
    "allure:report": "allure generate allure-results --clean -o allure-report"
  },
  "author": "jfc",
  "license": "ISC",
  "dependencies": {
    "cypress": "^12.7.0",
    "jsonfile": "^6.1.0",
    "mocha": "^10.2.0"
  },
  "plugins": ["cypress-xray-plugin"],
  "cypress-xray-plugin": {
    "xrayUrl": "https://your-jira-server.com/xray",
    "xrayProjectKey": "YOUR_XRAY_PROJECT_KEY",
    "xrayTestPlanKey": "YOUR_XRAY_TEST_PLAN_KEY"
  },
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.35.2",
    "chai": "^4.3.7",
    "cypress-multi-reporters": "^1.6.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "pdf-parse": "^1.1.1"
  }
}
